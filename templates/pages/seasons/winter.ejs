<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/Article">
    <%- include ../../partials/head %>
  <body class="<%= season %>-page">
    <%- include ../../partials/nav %>
    <%- include ../../partials/header %>
    <section class="left-side animated fadeInLeftBig">
      <h1 class="main-header">Winter<span>events</span></h1>
      <article class="blue-box">
        <p>"<%= data.winter.quotes.first.quote %>"</p>
        <p>-<%= data.winter.quotes.first.person %></p>
      </article>
      <article class="event-block kingman-moonlight">
        <img src="assets/logos/kingman-moonlight.png">
        <div class="event-info">
          <h2><%= data.winter.kingmanFarm.title %></h2>
          <h3><%= data.winter.kingmanFarm.date %></h3>
          <h3><%= data.winter.kingmanFarm.starttime %></h3>
          <h4><%= data.winter.kingmanFarm.address %></br>
              <%= data.winter.kingmanFarm.address2 %></br>
              <%= data.winter.kingmanFarm.city %>, <%= data.winter.kingmanFarm.state %></h4>
          <a href="kingman-farm-moonlight-snowshoe-race/"><button>click here for details</button></a>
          <h5>Photo Courtesy of <%= data.winter.kingmanFarm.photographer %></h5>
        </div>
      </article>
      <article class="event-block snowshoe-hullaballo">
        <img src="assets/logos/hullaballo.png">
        <div class="event-info">
          <h2><%= data.winter.snowshoeHullabaloo.title %></h2>
          <h3><%= data.winter.snowshoeHullabaloo.date %></h3>
          <h3><%= data.winter.snowshoeHullabaloo.starttime %></h3>
          <h4><%= data.winter.snowshoeHullabaloo.address %></br>
              <%= data.winter.snowshoeHullabaloo.address2 %></br>
              <%= data.winter.snowshoeHullabaloo.city %>, <%= data.winter.snowshoeHullabaloo.state %></h4>
          <a href="exeter-snowshoe-hullabaloo/"><button>click here for details</button></a>
          <h5>Photo Courtesy of <%= data.winter.snowshoeHullabaloo.photographer %></h5>
        </div>
      </article>
      <article class="event-block suunto">
        <img src="assets/logos/sunnto.jpg">
        <div class="event-info">
          <h2><%= data.suunto.title %></h2>
          <h3><%= data.suunto.copy %></h3>
        </div>
      </article>
    </section>

    <section class="right-side animated fadeInRightBig">
     
           
     <article class="event-block granite-state-snowshoe">
        <img src="assets/logos/gssc.jpg">
        <div class="event-info">
          <h2><%= data.winter.snowshoeChampionship.title %></h2>
          <h3><%= data.winter.snowshoeChampionship.date %></h3>
          <h3><%= data.winter.snowshoeChampionship.starttime %></h3>
          <h4><%= data.winter.snowshoeChampionship.address %></br>
              <%= data.winter.snowshoeChampionship.address2 %></br>
              <%= data.winter.snowshoeChampionship.city %>, <%= data.winter.snowshoeChampionship.state %></h4>
          <a href="nh-snowshoe-championship/"><button>click here for details</button></a>
          <h5>Photo Courtesy of <%= data.winter.snowshoeChampionship.photographer %></h5>
        </div>
      </article>    
      
      <article class="blue-box">
        <p>"<%= data.winter.quotes.second.quote %>"</p>
        <p>-<%= data.winter.quotes.second.person %></p>
      </article>
      <article class="event-block frosty-crank">
        <img src="assets/logos/frostycrank.png">
        <div class="event-info">
          <h2><%= data.winter.frostyCrank.title %></h2>
          <h3><%= data.winter.frostyCrank.date %></h3>
          <h3><%= data.winter.frostyCrank.starttime %></h3>
          <h4><%= data.winter.frostyCrank.address %></br>
              <%= data.winter.frostyCrank.address2 %></br>
              <%= data.winter.frostyCrank.city %>, <%= data.winter.frostyCrank.state %></h4>
          <a href="fat-bike-frosty-crank/"><button>click here for details</button></a>
          
        </div>
      </article>
      
    </section>

    <%- include ../../partials/footer %>
    <%- include ../../partials/scripts %>

  </body>
</html>
